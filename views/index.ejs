<%- include("partials/header") %>
    <!-- video container -->
    <header class="color-github">
        <%- include("partials/navbar") %>
            <section>
                <div id="container">
                    <div>
                        <div id="mainContent">
                            <h1 class="title">Hvordan hoste en nettside med GitHub</h1>
                            <div class="videoContent">
                                <video class="movie" controls>
                                    <source src="src/github.mp4" type="video/mp4">
                                </video>
                            </div>
                        </div>
                        <div class="hidden" id="secondaryContent">
                            <h1 class="title">Hvordan hoste en nettside med Heroku</h1>
                            <div class="videoContent">
                                <video class="movie" controls>
                                    <source src="src/Heroku.mp4" type="video/mp4">
                                </video>
                            </div>
                        </div>
                    </div>

                    <div id="slider">
                        <label class="switch">
                            <input type="checkbox" checked>
                            <span class="slider round"></span>
                        </label>
                    </div>
                </div>
            </section>
    </header>
    <section>
        <div id="mainText">
            <%- include("partials/githubTutorial") %>

        </div>
        <div class="hidden" id="secondaryText">
            <%- include("partials/herokuTutorial") %>
        </div>
    </section>


    <%- include("partials/footer") %>